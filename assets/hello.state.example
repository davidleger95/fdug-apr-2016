import React, { Component } from 'react';
import ReactDOM from 'react-dom';

class HelloMessage extends Component {
  constructor(props) {
    super(props);
    this.state = { name: 'David' };
  }
  render() {
    return (
      <div>
        Hello {this.state.name}
      </div>
    );
  }
}

ReactDOM.render(<HelloMessage />, document.body);
